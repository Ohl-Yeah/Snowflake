// Object dependency search
SELECT  *
FROM SNOWFLAKE.ACCOUNT_USAGE.OBJECT_DEPENDENCIES
WHERE REFERENCED_OBJECT_NAME = 'VW_BUDGET_FORECAST'
    OR REFERENCING_OBJECT_NAME = 'VW_BUDGET_FORECAST'
